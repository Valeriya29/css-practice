	<header class="header">
		<a href="#test">
				{{> logo}}
		</a>
		<nav class="header_nav">
			<div class="header_nav_right">
				{{> nav list=header}}
			</div>
		</nav>
	</header>
	
	<main class="main">
		{{#with info}}
			<div class="info">
				<h1 class="title">{{title}}</h1>
				<p class="description">{{description}}</p>
			</div>
		{{/with}}
		
		{{#with form}}
			<form class="form">
				<div class="form_input_list">
					{{#each inputs}}
						<input type="{{type}}" placeholder="{{placeholder}}" class="form_input" name="{{name}}" id="form-input-{{id}}"/>
					{{/each}}
				</div>

				<div class="form_checkbox">
					<input type="checkbox" name="{{checkbox.name}}" id="{{checkbox.name}}">
					<div></div>
					<label for="{{checkbox.name}}">{{checkbox.label}}</label>
				</div>

				<button type="submit" class="form_button">{{button}}</button>
				<div class="line"></div>
				<p class="form_info">{{info}}</p>
			</form>
		{{/with}}
		</main>

		{{#with updates}}
			<div class="updates">
				<h1 class="updates_title">{{title}}</h1>

				<div class="list">
				{{#each list}}
					<div class="square"></div>
						<span class="list_nickname">@{{nickname}}</span>
						<div class="list_description">
							<p>{{description}}</p>
						</div>
						
				{{/each}}
				</div>
				

				<a href="{{show_all.url}}" class="show_link">{{show_all.text}}</a>
			</div>
		{{/with}}

		<div class="expirience">
		{{#with expirience}}
				<div class="badge_expirience">{{badge}}</div>
				<h2 class="expirience_title">{{title}}</h2>

				<div class="list_expirience">
					{{#each list}}
						<div>
							<strong>{{name}}</strong>
							<p>{{details}}</p>
						</div>
					{{/each}}
				</div>	
		{{/with}}
		</div>
	

	<footer class="footer" id="test">
		<div>
				{{> logo}}
			<span class="footer_copyright">Copyright 2023-2024</span>
		</div>
				{{#each footer}}
		<div>
			<span class="footer_title">{{title}}</span>
				{{> nav list=links isVertical=true}}
		</div>
				{{/each}}
	</footer>
	

